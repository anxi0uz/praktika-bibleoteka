<Window x:Class="library_client.Windows.CreateUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:library_client.Windows"
        mc:Ignorable="d"
        Title="CreateUserWindow" Height="374" Width="390">
    <Grid>
        <TextBox x:Name="LoginTextBox" HorizontalAlignment="Left" Margin="150,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <TextBox x:Name="PasswordTextbox" HorizontalAlignment="Left" Margin="150,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <TextBox x:Name="FioTextBox" HorizontalAlignment="Left" Margin="150,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <TextBox x:Name="BirthTextBox" HorizontalAlignment="Left" Margin="150,203,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <TextBox x:Name="AdressTextBox" HorizontalAlignment="Left" Margin="150,251,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <TextBox x:Name="PhoneTextBox" HorizontalAlignment="Left" Margin="150,293,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220"/>
        <Button x:Name="CreateUserButton" Content="Сохранить" HorizontalAlignment="Left" Margin="10,316,0,0" VerticalAlignment="Top" Width="360" Click="CreateUserButton_Click"/>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <Label Content="Пароль" HorizontalAlignment="Left" Margin="10,117,0,0" VerticalAlignment="Top"/>
        <Label Content="ФИО" HorizontalAlignment="Left" Margin="10,149,0,0" VerticalAlignment="Top"/>
        <Label Content="Дата Рождения" HorizontalAlignment="Left" Margin="10,199,0,0" VerticalAlignment="Top" Height="43" Width="115"/>
        <Label Content="Адрес" HorizontalAlignment="Left" Margin="10,247,0,0" VerticalAlignment="Top"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="10,285,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="idTextBox" HorizontalAlignment="Left" Margin="150,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220" Visibility="Hidden"/>
        <Label x:Name="idlabel" Content="Id" HorizontalAlignment="Left" Margin="9,32,0,0" VerticalAlignment="Top" Visibility="Hidden"/>

    </Grid>
</Window>
